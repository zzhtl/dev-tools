{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "dev-tools",
  "version": "0.1.0",
  "identifier": "com.dev-tools.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:8927",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "dev-tools",
        "width": 1200,
        "height": 900
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}